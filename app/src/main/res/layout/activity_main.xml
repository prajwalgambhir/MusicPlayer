<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.beebrainy.servicesex.client.activities.MainActivity">

    <EditText
        android:id="@+id/edtPageNo"
        android:hint="Page No"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"/>

    <Button
        app:layout_constraintTop_toBottomOf="@id/edtPageNo"
        android:id="@+id/btnSubmit"
        android:text="SUBMIT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        app:layout_constraintTop_toBottomOf="@id/btnSubmit"
        android:id="@+id/btnLoggingService"
        android:text="LoggingService"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        app:layout_constraintTop_toBottomOf="@id/btnLoggingService"
        android:id="@+id/btnMusicService"
        android:text="MusicService"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


</android.support.constraint.ConstraintLayout>
