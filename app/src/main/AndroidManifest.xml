<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.beebrainy.servicesex">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".client.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <service android:name=".client.services.download.DownloadService"/>
        <service
            android:name=".client.services.misc.LogService"
            android:enabled="true"
            android:exported="true">
        </service>
        <service
            android:name=".client.services.misc.music.MusicService"
            android:enabled="true"
            android:exported="true">
        </service>
        <service
            android:name=".client.services.misc.music.MusicIntentService"
            android:exported="false">
        </service>

        <activity android:name=".client.activities.MusicActivity">
        </activity>
    </application>

</manifest>